<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.myapp391597" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>WINterviewApp</name>
  <description>
        WINterview mobile client application
    </description>
  <author email="jszeto0724@gmail.com" href="">
      Johnny Situ
    </author>
  <content src="index.html"/>
  <access origin="*"/>
